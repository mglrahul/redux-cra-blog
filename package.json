{
  "name": "redux-cra-blog",
  "version": "1.0.0",
  "description": "How to use create-react-app with a custom Node API on Heroku",
  "engines": {
    "node": "7.4.0"
  },
  "scripts": {
    "start": "nodemon --watch server --exec babel-node -- server",
    "heroku-postbuild": "cd react-ui/ && npm install --only=dev && npm install && npm run build"
  },
  "cacheDirectories": [
    "node_modules",
    "react-ui/node_modules"
  ],
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.4.7",
    "body-parser": "^1.15.2",
    "express": "^4.14.1",
    "jsonwebtoken": "^7.2.1",
    "mongoose": "^4.7.6",
    "mongoose-paginate": "^5.0.3",
    "validator": "^6.2.1",
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "nodemon": "^1.11.0"
  },
  "keywords": [
    "node",
    "heroku",
    "create-react-app",
    "react"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "nodemon": "^1.11.0"
  }
}
